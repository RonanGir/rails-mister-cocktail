<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <h1>New dose</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <%= simple_form_for [@cocktail, @dose] do |f| %>
      <%= f.association :ingredient %>
      <%= f.input :description %>
      <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
