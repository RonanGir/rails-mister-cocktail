<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <h1 class="text-center">Add a new cocktail</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-6">
      <%= simple_form_for @cocktail do |f| %>
      <%= f.input :name %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
